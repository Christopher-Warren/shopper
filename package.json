{
	"name": "shopper",
	"version": "0.1.0",
	"description": "shopping cart ui for shopify stores",
	"license": "ISC",
	"type": "module",
	"module": "dist/shopper.js",
	"main": "dist/shopper.js",
	"types": "dist/shopper.d.ts",
	"files": [
		"dist",
		"source"
	],
	"scripts": {
		"prepare": "run-s clean build test",
		"clean": "rimraf dist && mkdirp dist",
		"build": "run-p importmap compile copy-styles",
		"bundle": "rollup -c rollup.config.js",
		"importmap": "importly < package.json > dist/importmap.json",
		"compile": "tsc",
		"copy-styles": "ncp source/style.css dist",
		"watch": "tsc -w",
		"test": "exit 0",
		"start": "serve"
	},
	"jest": {
		"verbose": true,
		"testURL": "http://localhost/"
	},
	"dependencies": {
		"commotion": "0.1.0-dev.0",
		"crnc": "0.0.0-dev.18",
		"lit-element": "^2.2.1",
		"lit-html": "^1.1.2",
		"menutown": "0.0.1-dev.2",
		"shopify-buy": "^2.8.1"
	},
	"devDependencies": {
		"feather-icons": "^4.24.1",
		"importly": "0.1.0-dev.0",
		"mkdirp": "^0.5.1",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"octicons": "^8.5.0",
		"rimraf": "^3.0.0",
		"rollup": "^1.27.8",
		"rollup-plugin-node-resolve": "^5.2.0",
		"serve": "^11.2.0",
		"typescript": "^3.7.3"
	},
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"homepage": "https://github.com/chase-moskal/shopper#readme",
	"bugs": {
		"url": "https://github.com/chase-moskal/shopper/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/shopper.git"
	},
	"keywords": [
		"cart",
		"shopper",
		"shopping",
		"shopping-cart",
		"ecommerce",
		"shopify"
	]
}
