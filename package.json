{
	"name": "shopper",
	"version": "0.1.0-dev.9",
	"description": "shopping cart ui for shopify stores",
	"license": "ISC",
	"type": "module",
	"main": "dist/register-all.js",
	"module": "dist/register-all.js",
	"types": "dist/register-all.d.ts",
	"files": [
		"assets",
		"dist"
	],
	"scripts": {
		"prepare": "run-s clean build test",
		"clean": "rimraf dist && mkdirp dist",
		"build": "run-p importmap compile",
		"importmap": "importly < importly.config > dist/importmap.json",
		"compile": "tsc",
		"test": "exit 0",
		"start": "serve"
	},
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"keywords": [
		"cart",
		"shopper",
		"shopping",
		"shopping-cart",
		"ecommerce",
		"shopify"
	],
	"jest": {
		"verbose": true,
		"testURL": "http://localhost/"
	},
	"dependencies": {
		"lit-element": "2.2.0",
		"lit-html": "1.1.0",
		"menutown": "0.0.0-dev.23",
		"shopify-buy": "2.5.0"
	},
	"devDependencies": {
		"barrelsby": "2.1.1",
		"importly": "0.0.0-dev.8",
		"mkdirp": "0.5.1",
		"npm-run-all": "4.1.5",
		"rimraf": "2.6.3",
		"serve": "11.0.2",
		"typescript": "3.5.2"
	}
}
