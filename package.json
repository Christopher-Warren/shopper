{
	"name": "shopper",
	"version": "0.1.0-dev.11",
	"description": "shopping cart ui for shopify stores",
	"license": "ISC",
	"type": "module",
	"main": "dist/register-all.js",
	"module": "dist/register-all.js",
	"types": "dist/register-all.d.ts",
	"files": [
		"assets",
		"dist"
	],
	"scripts": {
		"prepare": "run-s clean build test",
		"clean": "rimraf dist && mkdirp dist",
		"build": "run-p importmap compile copy-styles",
		"importmap": "importly < package.json > dist/importmap.json",
		"compile": "tsc",
		"copy-styles": "ncp source/style.css dist",
		"watch": "tsc -w",
		"test": "exit 0",
		"start": "serve"
	},
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"keywords": [
		"cart",
		"shopper",
		"shopping",
		"shopping-cart",
		"ecommerce",
		"shopify"
	],
	"jest": {
		"verbose": true,
		"testURL": "http://localhost/"
	},
	"dependencies": {
		"lit-element": "2.2.1",
		"lit-html": "1.1.1",
		"menutown": "0.0.0-dev.27",
		"shopify-buy": "2.6.1"
	},
	"devDependencies": {
		"barrelsby": "2.1.1",
		"feather-icons": "4.24.0",
		"importly": "0.1.0-dev.0",
		"mkdirp": "0.5.1",
		"ncp": "2.0.0",
		"npm-run-all": "4.1.5",
		"octicons": "8.5.0",
		"rimraf": "2.6.3",
		"serve": "11.1.0",
		"typescript": "3.5.3"
	}
}
